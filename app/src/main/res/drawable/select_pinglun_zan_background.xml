<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <stroke android:color="@color/App_zan_background" android:width="1px"></stroke>
            <corners
                android:radius="3dp"/>
        </shape>
    </item>
    <item android:state_checked="false">
        <shape>
            <stroke android:color="@color/App_order_hint" android:width="1px"></stroke>
            <corners
                android:radius="3dp"/>
        </shape>
    </item>
</selector>