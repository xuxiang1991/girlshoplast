<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <solid android:color="@color/App_back_orange"/>
            <corners
                android:radius="16dp"/>
            <stroke android:color="@color/App_back_orange" android:width="1dp"></stroke>
        </shape>
    </item>
    <item android:state_checked="false">
        <shape>
            <solid android:color="@color/white"/>
            <corners
                android:radius="16dp"/>
            <stroke android:color="@color/App_back_orange" android:width="1dp"></stroke>
        </shape>
    </item>
</selector>